<h1>Listing file_list_items</h1>

<table>
  <tr>
    <th>Description</th>
    <th>File file name</th>
    <th>File content type</th>
    <th>File file size</th>
    <th>File updated at</th>
    <th>Position</th>
  </tr>

<% for file_list_item in @file_list_items %>
  <tr>
    <td><%=h file_list_item.description %></td>
    <td><%=h file_list_item.file_file_name %></td>
    <td><%=h file_list_item.file_content_type %></td>
    <td><%=h file_list_item.file_file_size %></td>
    <td><%=h file_list_item.file_updated_at %></td>
    <td><%=h file_list_item.position %></td>
    <td><%= link_to 'Show', file_list_item %></td>
    <td><%= link_to 'Edit', edit_file_list_item_path(file_list_item) %></td>
    <td><%= link_to 'Destroy', file_list_item, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New file_list_item', new_file_list_item_path %>
