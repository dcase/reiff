<h1>Listing bios</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Bio</th>
    <th>Photo file name</th>
    <th>Photo content type</th>
    <th>Photo file size</th>
    <th>Photo updated at</th>
  </tr>

<% for bio in @bios %>
  <tr>
    <td><%=h bio.name %></td>
    <td><%=h bio.email %></td>
    <td><%=h bio.bio %></td>
    <td><%=h bio.photo_file_name %></td>
    <td><%=h bio.photo_content_type %></td>
    <td><%=h bio.photo_file_size %></td>
    <td><%=h bio.photo_updated_at %></td>
    <td><%= link_to 'Show', bio %></td>
    <td><%= link_to 'Edit', edit_bio_path(bio) %></td>
    <td><%= link_to 'Destroy', bio, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New bio', new_bio_path %>
